{
  "name": "cdm-logger",
  "version": "3.0.0",
  "private": true,
  "description": "Universal - Logger",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "lerna": "lerna bootstrap --hoist",
    "install": "npm run lerna",
    "build": "tsc -p src/tsconfig.json -d",
    "publish": "npm run lerna:prepublish && lerna publish",
    "lerna:prepublish": "git checkout publish && git merge -s recursive -X theirs master",
    "publish:force": "npm run publish -- --force-publish=*",
    "debugtest": "npm run build && mocha --debug-brk",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "bunyan",
    "logger"
  ],
  "author": "Veera Marni <marni.srikanth@gmail.com> (http://vm.com)",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "lib"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*test*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "browser-bunyan": "^1.3.0",
    "bunyan": "^1.8.10",
    "bunyan-logstash-amqp": "1.0.7",
    "bunyan-prettystream": "^0.1.3"
  },
  "devDependencies": {
    "@types/browser-bunyan": "^0.4.4",
    "@types/bunyan": "^1.8.4",
    "@types/jest": "^20.0.0",
    "inversify": "^4.1.1",
    "jest": "^20.0.3",
    "jest-cli": "^20.0.4",
    "jquery": "^3.2.1",
    "lerna": "^2.0.0-rc.5",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.4",
    "ts-loader": "^2.3.2",
    "typescript": "^2.3.4",
    "webpack": "^3.4.1",
    "webpack-node-externals": "^1.6.0"
  },
  "homepage": "https://github.com/cdmbase/cdm-logger",
  "repository": {
    "type": "git",
    "url": "git://git@github.com:cdmbase/cdm-logger.git"
  },
  "bugs": {
    "url": "https://github.com/cdmbase/cdm-logger/issues"
  }
}
