{
  "name": "@cdm-logger/client",
  "version": "1.0.0",
  "description": "Universal - Logger",
  "main": "./lib/index.js",
  "type": "module",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "prepublish": "yarn build",
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "rimraf lib",
    "build:lib": "rollup -c rollup.config.mjs",
    "build:lib:watch": "yarn build:lib -- --watch",
    "jest": "./node_modules/.bin/jest",
    "test": "jest",
    "test:debug": "npm test -- --runInBand",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [
    "typescript",
    "bunyan",
    "logger"
  ],
  "author": "CDMBase LLC",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*test*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "@cdm-logger/core": "link:../core",
    "browser-bunyan": "~1.6.3",
    "stream": "0.0.2"
  },
  "homepage": "https://github.com/cdmbase/cdm-logger",
  "repository": {
    "type": "git",
    "url": "git://git@github.com:cdmbase/cdm-logger.git"
  },
  "bugs": {
    "url": "https://github.com/cdmbase/cdm-logger/issues"
  }
}
